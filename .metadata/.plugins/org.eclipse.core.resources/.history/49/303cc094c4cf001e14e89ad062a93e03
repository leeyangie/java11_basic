package org.kh.site;
//관계(비교) 연산자 : ==, !=, >, >=, <, <= / true or false 로 나옴
public class Operation5 {
	public static void main(String[] args) {
		int a = 25;
		int b = 28;
		int c = 27;
		System.out.println(a==b);
		System.out.println(a!=b);
		System.out.println(a>b);
		System.out.println(a>=b);
		System.out.println(a>c);
	}

}
